    "url": "https://github.com/natsuki-yasumi/aplikasi-komik-anekdot/issues"
  },
  "homepage": "https://github.com/natsuki-yasumi/aplikasi-komik-anekdot#readme",
  "dependencies": {
    "babel-preset": "^1.1.7",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/register": "^7.23.7",
    "babel-jest": "^29.7.0",
